---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>The Spoken Rune</title>
 <link href="http://spokenrune.com/atom.xml" rel="self"/>
 <link href="http://spokenrune.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://spokenrune.com/</id>
 <author>
   <name>Cameron Higby-Naquin</name>
   <email>cameron.higbynaquin@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://spokenrune.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://spokenrune.com{{ post.id }}</id>
   <content type="html">"{{ post.title | xml_escape }}" by {{ post.author | xml_escape }}&lt;br /&gt;&lt;br /&gt;
   &lt;a href="https://s3.amazonaws.com/spokenrune/{{ post.filename }}.mp3"&gt;Download&lt;/a&gt;
   </content>
 </entry>
 {% endfor %}

</feed>